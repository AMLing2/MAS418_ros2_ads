<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1">
  <POU Name="MAIN" Id="{e46c70c8-013b-48fc-b994-d62a933e7122}" SpecialFunc="None">
    <Declaration><![CDATA[PROGRAM MAIN
VAR
	stButtonInputs : ST_in_digital;
	stAnalogInputs : ST_inputs_analog;
	stOutputs : ST_ValvePos;
	eCraneMode : E_CraneStatus;
	ipControlMode : I_craneControl;
	fbManual : FB_manualControl;
	fbAuto : FB_AutoControl;
	stValveOut : ST_ValvePos;
	fbIOsim : FB_ioSim;
	fBoomAng : LREAL;
	fBoomAngRef : LREAL;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[eCraneMode := F_stateMachine(stButtonInputs);
CASE stButtonInputs.eModeSwitch OF //polymorphism using interface pointer ipControlMode
		E_CraneModes.Manual:
			ipControlMode := fbManual;
		E_CraneModes.Auto:
			ipControlMode := fbAuto;
END_CASE

IF eCraneMode = E_CraneStatus.Running AND ipControlMode <> 0  THEN
	fbIOsim.getOutputs(stSimOut := stAnalogInputs);
	ipControlMode.controller(stButtonInputs := stButtonInputs,
		stAnalogInputs := stAnalogInputs,
		stOutput => stValveOut);
	fbIOsim.setInputs(stValvePos := stValveOut,
		eCraneState := eCraneMode);
END_IF

fBoomAng := F_CalcAngle(stAnalogInputs.fPistonPos);
]]></ST>
    </Implementation>
  </POU>
</TcPlcObject>